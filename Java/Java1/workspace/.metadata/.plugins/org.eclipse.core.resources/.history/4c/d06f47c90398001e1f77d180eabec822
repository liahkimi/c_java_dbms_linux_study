package day01;

public class PrintTest { // 클래스 영역 시작
	
	public static void main(String[] args) { // 메인 메소드 영역 시작

		// 출력 메소드
		// 정렬 단축키 : ctrl + shift + f
		// 주석 단축키 : ctrl + /
		//System.out.println("hello world");

		// 범위 주석 단축키 설정 : ctrl + shift + /
		// 범위 주석 단축키 해제 : ctrl + shift + \
		/*
		 * 범위 주석 시작 System.out.println("bye"); System.out.println("bye");
		 * System.out.println("bye"); System.out.println("bye");
		 * System.out.println("bye"); System.out.println("bye");
		 * System.out.println("bye"); 범위 주석 끝
		 */

	} // 메인 메소드 영역 끝

} // 클래스 영역 끝
